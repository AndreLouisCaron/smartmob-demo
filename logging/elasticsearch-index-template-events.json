{
    "template": "events-*",
    "mappings": {
        "_default_": {
            "properties": {
                "@timestamp": {
                    "type": "date",
                    "index": "not_analyzed"
                }
            }
        },
        "events": {
            "properties": {
                "service": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "event": {
                    "type": "string",
                    "index": "not_analyzed"
                }
            }
        }
    }
}
